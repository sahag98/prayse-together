{
  "expo": {
    "name": "Bible Study",
    "slug": "prayse-together",
    "version": "1.0.0",
    "newArchEnabled": true,
    "scheme": "prayse-together",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FAF9F6",
          "image": "./assets/prayse-together-logo.png",
          "dark": {
            "image": "./assets/prayse-together-logo.png",
            "backgroundColor": "#FAF9F6"
          },
          "imageWidth": 200
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "sahag",
          "project": "react-native-oi",
          "url": "https://sentry.io/"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you setup a profile picture."
        }
      ],
      ["expo-apple-authentication"],
      ["expo-router"],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-image.png",
      "resizeMode": "contain",
      "backgroundColor": "#F2F0E4"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.sahag98.praysetogether"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sahag98.praysetogether",
      "permissions": ["android.permission.RECORD_AUDIO", "android.permission.RECORD_AUDIO"]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "47f58c9f-8bbb-4f7e-81c1-4aaa9ef1fce5"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/47f58c9f-8bbb-4f7e-81c1-4aaa9ef1fce5"
    }
  }
}
